{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "module": "esnext",
        "moduleResolution": "nodenext",
        "sourceMap": true,
        "strict": true,
        "outDir": "out",
        "rootDir": "src",
        "skipLibCheck": true,
		"esModuleInterop": true,
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}
